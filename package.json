{
  "name": "marky-markdown",
  "version": "2.2.1",
  "description": "The thing npm uses to clean up READMEs and other markdown files",
  "tagline": "It's like ordinary markdown except it drops its pants for attention.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "doc": "readme package.json --test > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/marky-markdown"
  },
  "keywords": [
    "readme",
    "markdown",
    "md",
    "documentation",
    "syntax highlighting",
    "html",
    "frontmatter",
    "github",
    "npm"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/npm/marky-markdown/issues"
  },
  "homepage": "https://github.com/npm/marky-markdown",
  "dependencies": {
    "cheerio": "^0.18.0",
    "github-url-to-object": "^1.4.2",
    "highlights": "^0.15.0",
    "html-frontmatter": "^1.2.1",
    "js-beautify": "^1.5.4",
    "lodash": "^2.4.1",
    "marked": "^0.3.2",
    "pretty": "^1.0.0",
    "sanitize-html": "^1.4.3",
    "similarity": "^1.0.1"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "benchmark": "^1.0.0",
    "cicada": "~1.1.1",
    "express": "^4.10.7",
    "flake": "^1.0.0",
    "grunt-angular-templates": "^0.5.7",
    "johnny-five": "^0.8.37",
    "memoize": "~0.1.1",
    "mkhere": "~1.0.9",
    "mocha": "^2.0.1",
    "package-json-to-readme": "^1.3.0",
    "wzrd": "^1.1.1"
  },
  "bin": "./bin/marky-markdown.js"
}
